<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.umich.flowfence.study.caminjector">

    <uses-permission android:name="edu.umich.flowfence.permission.BIND_FLOWFENCE_SERVICE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CamInjectorMain"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".CamInjector"
            android:enabled="true"
            android:exported="false"></service>

        <meta-data android:name="edu.umich.flowfence.manifest" android:resource="@xml/flowfence_manifest"/>

    </application>



</manifest>
